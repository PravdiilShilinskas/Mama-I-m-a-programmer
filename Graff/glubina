#include <iostream>
using namespace std;

void glubina(int* evo, int* evd, int e, int k, int v){
if(v>1)
   for(int i=0;i<e;i++){
      if(evo[i]==k){
         cout << i+1 << "  " << "  " << "  ";
         v--;
         glubina(evo, evd, e, evd[i], v);
      };
   };
}

void Graff(int e, int v){
   int evo[e];
   int evd[e];
   
   for(int i=0;i<e;i++){
      cin >> evo[i] >> evd[i];
   };
   glubina(evo, evd, e, 1, v);
}

int main(){
   int e;
   int v;
   cin >> e;
   cin >> v;
   Graff(e, v);
}
